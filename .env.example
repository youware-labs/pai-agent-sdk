# pai-agent-sdk Environment Variables
# Copy this file to .env and fill in your values

# =============================================================================
# Web Search API Keys
# =============================================================================

# Google Custom Search API
# Get your key at: https://developers.google.com/custom-search/v1/introduction
GOOGLE_SEARCH_API_KEY=
GOOGLE_SEARCH_CX=

# Tavily Search API
# Get your key at: https://tavily.com/
TAVILY_API_KEY=

# =============================================================================
# Image Search API Keys
# =============================================================================

# Pixabay API (stock images)
# Get your key at: https://pixabay.com/api/docs/
PIXABAY_API_KEY=

# RapidAPI (real-time image search)
# Get your key at: https://rapidapi.com/
RAPIDAPI_API_KEY=

# =============================================================================
# Web Scraping API Keys
# =============================================================================

# Firecrawl API
# Get your key at: https://firecrawl.dev/
FIRECRAWL_API_KEY=

# =============================================================================
# Agent Configuration (PAI_AGENT_ prefix)
# =============================================================================

# Working directory for tool path validation (default: current working directory)
PAI_AGENT_WORKING_DIR=

# Base directory for session temporary files (default: system temp)
PAI_AGENT_TMP_BASE_DIR=

# Model for image understanding when native vision is unavailable
PAI_AGENT_IMAGE_UNDERSTANDING_MODEL=

# Model for video understanding when native capability is unavailable
PAI_AGENT_VIDEO_UNDERSTANDING_MODEL=

# Model for compact when native capability is unavailable
PAI_AGENT_COMPACT_MODEL=

# =============================================================================
# Browser Use Configuration (PAI_AGENT_BROWSER_USE_ prefix)
# =============================================================================

# Maximum retry attempts for tool calls (default: 3)
PAI_AGENT_BROWSER_USE_MAX_RETRIES=

# Tool name prefix (default: none)
PAI_AGENT_BROWSER_USE_PREFIX=

# Force create new page instead of reusing existing (default: false)
PAI_AGENT_BROWSER_USE_ALWAYS_USE_NEW_PAGE=

# Automatically close created page targets on context exit (default: false)
PAI_AGENT_BROWSER_USE_AUTO_CLEANUP_PAGE=
